        "launch": "react-scripts start",
        "build:dev": "env-cmd -f .env.development react-scripts build && yarn run postbuild",
        "build:prod": "env-cmd -f .env.production react-scripts build && yarn run postbuild",
        "postbuild": "copyfiles -f src/auth/auth.html build/",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "dev": "env-cmd -f .env.development react-scripts start",
        "format": "prettier --write --config src/configs/prettierConfig.json --ignore-path src/configs/.ignore",
        "format-all": "yarn run format \"src/**/*.{js,jsx,ts,tsx,json,css,scss,less}\"",
